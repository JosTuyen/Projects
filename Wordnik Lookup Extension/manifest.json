{
  "name": "Getting started example",
  "manifest_version": 2,
  "version": "1.0",

  "description": "Check out the staff stack more convenience.",
  "icons":{
    "128":"icons/128.png",
    "48":"icons/48.png",
    "16":"icons/16.png"
  },
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js":["content.js"]
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Lookup a definition."
  }
}
